
public class Cat {
	private String name;
	private int age;
	
	public Cat(String name, int age){
		this.name = name;
		this.age = age;
	}
	
	public String toString(){
		return "Cat named " + name + " is " + age + " years old.";
	}
	
	public void meow(){
		System.out.println("Meow!");
	}
	
	public void hiss(){
		System.out.println("Hiss! Hiss! *" + name + "doesn't like you*");
	}
	
	public void purr(){
		System.out.println("Purrrrr... :3");
	}
	
	public void eat(){
		System.out.println("Nom nom nom... *cat eats*");
	}
	
	public void drink(){
		System.out.println("Lap lap lap... *cat drinks*");
	}
}
